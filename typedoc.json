{
  "$schema": "https://typedoc.org/schema.json",
  "cleanOutputDir": false,
  "customCss": "./build-utils/typedoc-custom.css",
  "darkHighlightTheme": "dark-plus",
  "entryPoints": ["lib/buffer.ts"],
  "gaID": "G-KB8ZL6LPH5",
  "includeVersion": true,
  "out": "dist",
  "projectDocuments": ["documents/*.md"],
  "plugin": [
    "typedoc-plugin-ga", 
    "typedoc-plugin-mdn-links", 
    "typedoc-plugin-missing-exports", 
    "typedoc-plugin-rename-defaults"
  ],
  "navigationLinks": {
    "GitHub": "https://github.com/taichunmin/js-buffer",
    "NPM": "https://www.npmjs.com/package/@taichunmin/buffer",
    "jsDelivr": "https://www.jsdelivr.com/package/npm/@taichunmin/buffer"
  },
  "groupOrder": [
    "Constructors",
    "Static Methods",
    "Methods",
    "Alias Methods",
    "*"
  ]
}